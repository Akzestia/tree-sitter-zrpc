{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "zrpc",
      "camelcase": "Zrpc",
      "title": "Zrpc",
      "scope": "source.zrpc",
      "file-types": [
        "zrpc"
      ],
      "injection-regex": "^zrpc$",
      "class-name": "TreeSitterZrpc"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Zrpc tree-sitter",
    "authors": [
      {
        "name": "アクゼスティア",
        "email": "akzestia@zurui.io"
      }
    ],
    "links": {
      "repository": "https://github.com/Akzestia/tree-sitter-zrpc.git"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}